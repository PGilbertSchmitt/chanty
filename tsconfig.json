{
  "compilerOptions": {
    "target": "ES2017",
    "module": "None",
    "moduleResolution": "Node",
    "lib": [
      "ES2019"
    ],
    "allowJs": false,
    "declaration": true,
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "downlevelIteration": true
  }
}